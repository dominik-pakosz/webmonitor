<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        
        {% block stylesheets %} 
        <link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}">        
        <link rel="stylesheet" href="{{ asset('assets/vendor/jqueryui/jquery-ui.css') }}">        
        <link rel="stylesheet" href="{{ asset('assets/vendor/jqueryui/jquery-ui.theme.css') }}">        
        <link rel="stylesheet" href="{{ asset('assets/vendor/jqueryui/jquery-ui.structure.css') }}">
        <link rel="stylesheet" href="/css/webmonitor.css">
        {% endblock %}
        

    </head>
    <body>
        {% block body %}{% endblock %}
        
        {% block javascripts %}
        <script src="{{ asset('assets/vendor/jquery/dist/jquery.js') }}"></script>
        <script src="{{ asset('assets/vendor/bootstrap/dist/js/bootstrap.js') }}"></script>
        <script src="{{ asset('assets/vendor/jqueryui/jquery-ui.js') }}"></script>
        {% endblock %}
    </body>
</html>
